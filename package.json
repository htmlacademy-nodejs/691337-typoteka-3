{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "test": "npm run eslint",
    "test::rest": "jest .e2e.test.js",
    "eslint": "eslint --ext .js src/",
    "start": "concurrently --names \"API, SSR\" --prefix-colors \"green, red\" \"npm:run:api\" \"npm:run:ssr\"",
    "run:api": "node ./src/service/service.js --server",
    "run:ssr": "node ./src/express/index.js",
    "start::debug": "cross-env NODE_ENV=development LOG_LEVEL=debug nodemon ./src/service/service.js --server || pino-pretty",
    "start-front-server": "nodemon ./src/express/index.js",
    "fill-db": "node ./src/service/service.js --filldb 20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honiara1973/691337-typoteka-3.git"
  },
  "author": "Anna T",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/honiara1973/691337-typoteka-3/issues"
  },
  "homepage": "https://github.com/honiara1973/691337-typoteka-3#readme",
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.5.2",
    "jest": "25.4.0",
    "nodemon": "^2.0.4",
    "pino-pretty": "4.0.0",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "5.0.0",
    "chalk": "3.0.0",
    "concurrently": "6.0.2",
    "cookie-parser": "1.4.5",
    "cross-env": "7.0.2",
    "csurf": "1.11.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "joi": "17.3.0",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "3.1.2",
    "moment": "2.29.1",
    "multer": "1.4.2",
    "nanoid": "2.1.11",
    "pg": "8.3.0",
    "pino": "6.2.1",
    "pug": "3.0.2",
    "sequelize": "6.3.4",
    "socket.io": "4.1.2"
  }
}
